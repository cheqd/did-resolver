                        {
                            "$ref": "#/definitions/types.ContentType"
                        }
                    ],
                    "example": "application/did+ld+json"
                },
                "did": {
                    "$ref": "#/definitions/types.DidProperties"
                },
                "error": {
                    "type": "string"
                },
                "retrieved": {
                    "type": "string",
                    "example": "2021-09-01T12:00:00Z"
                }
            }
        },
        "types.Service": {
            "type": "object",
            "properties": {
                "@context": {
                    "type": "array",
                    "items": {
                        "type": "string"
                    }
                },
                "id": {
                    "type": "string",
                    "example": "did:cheqd:testnet:55dbc8bf-fba3-4117-855c-1e0dc1d3bb47#service-1"
                },
                "serviceEndpoint": {
                    "type": "string",
                    "example": "https://example.com/endpoint/8377464"
                },
                "type": {
                    "type": "string",
                    "example": "did-communication"
                }
            }
        },
        "types.VerificationMethod": {
            "type": "object",
            "properties": {
                "@context": {
                    "type": "array",
                    "items": {
                        "type": "string"
                    }
                },
                "controller": {
                    "type": "string"
                },
                "id": {
                    "type": "string"
                },
                "publicKeyJwk": {
                    "type": "object",
                    "additionalProperties": {
                        "type": "string"
                    }
                },
                "publicKeyMultibase": {
                    "type": "string"
                },
                "type": {
                    "type": "string"
                }
            }
        }
    }
}